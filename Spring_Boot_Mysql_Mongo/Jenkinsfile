pipeline {
    agent any
    tools {
        maven 'maven' 
    }
    stages {
        stage('clean') {
            steps {
                sh 'mvn --version'
                sh 'cd Spring_Boot_Mysql_Mongo && mvn clean'
            }
        }
         stage('build') {
            steps {
                sh 'mvn --version'
                sh 'cd Spring_Boot_Mysql_Mongo && mvn clean install'
            }
        }
    }
}
